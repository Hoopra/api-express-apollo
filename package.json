{
  "name": "api",
  "version": "0.0.1",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "cross-env ENV_FILE=production node -r ./env -r module-alias/register ./dist/start.js",
    "start-dev": "nodemon --config \"./util/nodemon.json\"",
    "test": "nodemon --config \"./util/nodemon.test.json\""
  },
  "_moduleAliases": {
    "@model": "dist/model",
    "@server": "dist/server",
    "@util": "dist/util",
    "@apollo": "dist/apollo"
  },
  "dependencies": {
    "@types/graphql": "14.5.0",
    "apollo-server": "2.9.0",
    "apollo-server-express": "2.9.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "1.4.4",
    "cross-env": "5.2.0",
    "dotenv": "8.1.0",
    "easygraphql-tester": "^5.1.6",
    "express": "4.17.1",
    "graphql": "14.5.3",
    "http-status-codes": "1.3.2",
    "jsonwebtoken": "^8.5.1",
    "massive": "^5.11.2",
    "module-alias": "2.2.1",
    "morgan": "1.9.1",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "1.4.2",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.1",
    "@types/find": "0.2.1",
    "@types/jasmine": "3.4.0",
    "@types/jsonfile": "5.0.0",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/massive": "^5.4.3",
    "@types/morgan": "1.7.37",
    "@types/node": "12.7.2",
    "@types/supertest": "2.0.8",
    "find": "0.3.0",
    "fs-extra": "8.1.0",
    "jasmine": "3.4.0",
    "jsonfile": "5.0.0",
    "nodemon": "1.19.1",
    "supertest": "4.0.2",
    "ts-node": "8.3.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.19.0",
    "typescript": "3.5.3"
  }
}
